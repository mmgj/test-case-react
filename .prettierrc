{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "arrowBodyStyle": "as-needed",
  "react/jsx-max-props-per-line": [1, { "when": "always" }]
}
